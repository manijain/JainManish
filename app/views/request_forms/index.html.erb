<h1>Listing request_forms</h1>

<div class= "row">

  <table class="table table-striped">
    <thead>
      <tr>
        <th>Name</th>
        <th>Email</th>
        <th>Message</th>
        <th colspan="3"></th>
      </tr>
    </thead>

    <tbody>
      <% @request_forms.each do |request_form| %>
        <tr>
          <td><%= request_form.name %></td>
          <td><%= request_form.email %></td>
          <td><%= request_form.message %></td>
          <td><%= link_to 'Show', request_form %></td>
          <td><%= link_to 'Edit', edit_request_form_path(request_form) %></td>
          <td><%= link_to 'Destroy', request_form, method: :delete, data: { confirm: 'Are you sure?' } %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
  <br>
</div>